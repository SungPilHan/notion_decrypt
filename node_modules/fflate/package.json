{
  "name": "fflate",
  "version": "0.7.4",
  "description": "High performance (de)compression in an 8kB package",
  "main": "./lib/index.cjs",
  "module": "./esm/browser.js",
  "types": "./lib/index.d.ts",
  "unpkg": "./umd/index.js",
  "jsdelivr": "./umd/index.js",
  "browser": {
    "./lib/node-worker.cjs": "./lib/worker.cjs"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "node": {
        "import": "./esm/index.mjs",
        "require": "./lib/node.cjs"
      },
      "import": "./esm/browser.js",
      "require": "./lib/index.cjs"
    },
    "./node": {
      "types": "./lib/index.d.ts",
      "import": "./esm/index.mjs",
      "require": "./lib/node.cjs"
    },
    "./browser": {
      "types": "./lib/index.d.ts",
      "import": "./esm/browser.js",
      "require": "./lib/browser.cjs"
    }
  },
  "targets": {
    "main": false,
    "module": false,
    "browser": false,
    "types": false
  },
  "sideEffects": false,
  "homepage": "https://101arrowz.github.io/fflate",
  "repository": "https://github.com/101arrowz/fflate",
  "author": "Arjun Barrett <arjunbarrett@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/pako": "*",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "jszip": "^3.5.0",
    "pako": "*",
    "parcel": "^2.0.0-nightly.440",
    "parcel-config-precache-manifest": "^0.0.3",
    "preact": "^10.5.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "simple-git": "^2.22.0",
    "terser": "^5.3.8",
    "tiny-inflate": "*",
    "ts-node": "^9.0.0",
    "typedoc": "^0.17.0-3",
    "typedoc-plugin-markdown": "^3.0.2",
    "typescript": "^4.0.2",
    "uvu": "^0.3.3",
    "uzip": "*"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react-dom/test-utils": "preact/test-utils"
  }
}